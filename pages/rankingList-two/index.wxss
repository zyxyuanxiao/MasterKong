/**index.wxss**/
.container{
  color: #FFFFFF;
}
.ranking{
  width: 100%;
  height: 4.93%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.rankOne{
  width: 53.33%;
  height: 4.93%;
  text-align: center;
  line-height: 70rpx;
  display: flex;
  background:#3c3c46;
  border-radius:40px;
}
.OnTheList{
  width: 50%;
  height: 100%;
}
.TheTotalList{
  width: 50%;
  height: 100%;
  background:rgba(31,204,102,1);
  border-radius:40px;
}
.list{
  width: 92%;
  height: 62.25%;
  background:rgba(47,47,57,1);
  border-radius:8px;
  margin: 30rpx 4.4% 0rpx 3.6%;
}
.MyRank{
  width: 100%;
  height: 14.35%;
