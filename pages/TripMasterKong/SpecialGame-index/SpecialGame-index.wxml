          <image src="{{img_url}}/assets/images/TripMasterKong/ico／定位点@2x.png" class="point"></image>
          <image src="{{img_url}}/assets/images/TripMasterKong/ico／锁@2x.png" class="lock"></image>
        </view>
        <!-- 第二个 -->
